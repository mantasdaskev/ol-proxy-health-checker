<Application
    x:Class="Ol.ProxyHealthChecker.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:lib="clr-namespace:Ol.ProxyHealthChecker.Lib.UI">
    <Application.Resources>
        <lib:BoolToBrushConverter
            x:Key="BoolToBrushConverter"
            HealthyBrush="LimeGreen"
            UnhealthyBrush="Tomato" />
    </Application.Resources>
</Application>
